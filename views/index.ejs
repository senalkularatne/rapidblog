<% include ./partials/header %>

<h1>Index Page</h1>

<% blogs.forEach( function(blog) { %>

	<div>
		<h2> <%= blog.title %> </h2>
		<img src="<%= blog.image %>" >
		<span> <%= blog.dateCreated %> </span>
		<p> <%= blog.body.substring(0, 110) %> ...</p>
		<a href="/blogs/<%= blog._id %>">More Info</a>
	</div>

<% }); %>

<% include ./partials/footer %>
